{
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontWeight": "bold",
  "workbench.colorCustomizations": {
    "terminal.foreground": "#0f9bbe",
    "terminalCursor.foreground": "#fa8e01",
    "terminal.selectionBackground": "#000"
  },
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.cursorWidth": 2,
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira code",
  "editor.fontWeight": "600",
  "editor.fontSize": 20,
  "editor.inlayHints.enabled": "off",
  "workbench.iconTheme": "material-icon-theme",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorWidth": 3,
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.js": "javascriptreact"
  },
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "explorer.confirmDelete": false,
  "editor.parameterHints.enabled": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.tabs.location": "left",
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/*.class": true,
    "**/node_modules/": true
  },
  "editor.formatOnSave": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "git.confirmSync": false,
  "workbench.colorTheme": "Bearded Theme Black & Diamond",
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "cSpell.userWords": [
    "Catergory",
    "Datagram",
    "fileupload",
    "postbuild",
    "Reciever",
    "Servlet"
  ],
  "java.jdt.ls.java.home": "c:\\Program Files\\Java\\jdk-17.0.1",
  "redhat.telemetry.enabled": true,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "emulator.emulatorPath": "C:\\Users\\user\\AppData\\Local\\Android\\Sdk\\emulator", // change user to user-name
  "emulator.emulatorPathWindows": "C:\\Users\\user\\AppData\\Local\\Android\\Sdk\\emulator",
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "javascriptreact": "node"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "npm.keybindingsChangedWarningShown": true
}
